    <string-array name="Al_Kafirun">
        <item>بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ</item>
        <item>قُلْ يَٰٓأَيُّهَا ٱلْكَٰفِرُونَ</item>
        <item>لَآ أَعْبُدُ مَا تَعْبُدُونَ</item>
        <item>وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُ</item>
        <item>وَلَآ أَنَا۠ عَابِدٌۭ مَّا عَبَدتُّمْ</item>
        <item>وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُ</item>
        <item>لَكُمْ دِينُكُمْ وَلِىَ دِينِ</item>
    </string-array>

    <string-array name="Al_Nasr">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>إِذَا  جَاءَ  نَصْرُ  اللَّهِ  وَالْفَتْحُ</item>
        <item>وَ  رَأَيْتَ  النَّاسَ  يَدْخُلُونَ  فِي  دِينِ  اللَّهِ  أَفْوَاجًا</item>
        <item>فَسَبِّحْ  بِحَمْدِ  رَبِّكَ  وَاسْتَغْفِرْهُۚ  إِنَّهُ  كَانَ  تَوَّابًا</item>
    </string-array>


    <string-array name="Al_Masad">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>تَبَّتْ يَدَآ أَبِى لَهَبٍۢ وَتَبَّ</item>
        <item>مَآأَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ</item>
        <item>سَيَصْلَىٰ نَارًۭا ذَاتَ لَهَبٍۢ</item>
        <item>وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ</item>
        <item>فِى جِيدِهَا حَبْلٌۭ مِّن مَّسَدٍۭ</item>
    </string-array>


