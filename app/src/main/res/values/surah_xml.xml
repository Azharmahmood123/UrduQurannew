    <!-- Surahs arabic -->
    <string-array name="Al_Fatihah">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>ٱلۡحَمۡدُ لِلَّهِ رَبِّ ٱلۡعَـٰلَمِينَ</item>
        <item>ٱلرَّحۡمَـٰنِ ٱلرَّحِيم</item>
        <item>مَـٰلِكِ يَوۡمِ ٱلدِّينِ </item>
        <item>إِيَّاكَ نَعۡبُدُ وَإِيَّاكَ نَسۡتَعِينُ</item>
        <item>ٱهۡدِنَا ٱلصِّرَٲطَ ٱلۡمُسۡتَقِيمَ</item>
        <item>صِرَٲطَ ٱلَّذِينَ أَنۡعَمۡتَ عَلَيۡهِمۡ غَيۡرِ ٱلۡمَغۡضُوبِ عَلَيۡهِمۡ وَلَا ٱلضَّآلِّينَ </item>
    </string-array>

    <string-array name="Ash_Sharah">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ</item>
        <item>وَوَضَعْنَا عَنكَ وِزْرَكَ</item>
        <item>الَّذِي
أَنقَضَ
ظَهْرَكَ</item>
        <item>وَرَفَعْنَا لَكَ ذِكْرَكَ</item>
        <item>فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا</item>
        <item>إِنَّ مَعَ ٱلْعُسْرِ يُسْرًۭا</item>
        <item>فَإِذَا فَرَغْتَ فَٱنصَبْ</item>
        <item>وَإِلَىٰ رَبِّكَ فَٱرْغَب</item>
    </string-array>

    <string-array name="At_Tin">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>وَٱلتِّينِ وَٱلزَّيْتُونِ</item>
        <item>وَطُورِ سِينِينَ</item>
        <item>وَهَٰذَا ٱلْبَلَدِ ٱلْأَمِينِ</item>
        <item>لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍۢ</item>
        <item>ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ</item>
        <item>إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۢ</item>
        <item>فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ</item>
        <item>أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَٰكِمِينَ</item>
    </string-array>

    <string-array name="Al_Qadr">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةِ ٱلْقَدْرِ</item>
        <item>وَمَآ أَدْرَاكَ مَا لَيْلَةُ ٱلْقَدْرِ</item>
        <item>لَيْلَةُ ٱلْقَدْرِ خَيْرٌۭ مِّنْ أَلْفِ شَهْرٍۢ</item>
        <item>تَنَزَّلُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٍۢ</item>
        <item>سَلَٰمٌ هِىَ حَتَّىٰ مَطْلَعِ ٱلْفَجْرِ</item>
    </string-array>



    <string-array name="At_Takathur">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>أَلْهَىٰكُمُ ٱلتَّكَاثُرُ</item>
        <item>حَتَّىٰ زُرْتُمُ ٱلْمَقَابِرَ</item>
        <item>كَلَّا سَوْفَ تَعْلَمُونَ</item>
        <item>ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ</item>
        <item>كَلَّا لَوْ تَعْلَمُونَ عِلْمَ ٱلْيَقِينِ</item>
        <item>لَتَرَوُنَّ ٱلْجَحِيمَ</item>
        <item>ثُمَّ لَتَرَوُنَّهَا عَيْنَ ٱلْيَقِينِ</item>
        <item>ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ ٱلنَّعِيمِ</item>
    </string-array>


    <string-array name="Al_Asr">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>وَٱلْعَصْرِ</item>
        <item>إِنَّ ٱلْإِنسَٰنَ لَفِى خُسْرٍ</item>
        <item>إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ وَتَوَاصَوْا۟ بِٱلْحَقِّ وَتَوَاصَوْا۟ بِٱلصَّبْرِ</item>
    </string-array>


    <string-array name="Al_Fil">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>ألَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ ٱلْفِيلِ</item>
        <item>أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍۢ</item>
        <item>وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ</item>
        <item>تَرْمِيهِم بِحِجَارَةٍۢ مِّن سِجِّيلٍۢ</item>
        <item>فَجَعَلَهُمْ كَعَصْفٍۢ مَّأْكُولٍۭ</item>
    </string-array>

    <string-array name="Al_Quraish">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>لِإِيلَٰفِ قُرَيْشٍ</item>
        <item>إِۦلَٰفِهِمْ رِحْلَةَ ٱلشِّتَآءِ وَٱلصَّيْفِ</item>
        <item>فَلْيَعْبُدُوا۟ رَبَّ هَٰذَا ٱلْبَيْتِ</item>
        <item>ٱلَّذِىٓ أَطْعَمَهُم مِّن جُوعٍۢ وَءَامَنَهُم مِّنْ خَوْفٍۭ</item>
    </string-array>


    <string-array name="Al_Maun">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ</item>
        <item>فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ</item>
        <item>وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ</item>
        <item>فَوَيْلٌۭ لِّلْمُصَلِّينَ</item>
        <item>ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ</item>
        <item>ٱلَّذِينَ هُمْ يُرَآءُونَ</item>
        <item>وَيَمْنَعُونَ ٱلْمَاعُونَ</item>
    </string-array>

    <string-array name="Al_Kauthar">
        <item> بِسْمِ اللَّـهِ الرَّحْمَـٰنِ الرَّحِيمِ</item>
        <item>إِنَّآ أَعْطَيْنَٰكَ ٱلْكَوْثَرَ</item>
        <item>فَصَلِّ لِرَبِّكَ وَٱنْحَرْ</item>
        <item>إِنَّ شَانِئَكَ هُوَٱلْأَبْتَرُ</item>
    </string-array>

